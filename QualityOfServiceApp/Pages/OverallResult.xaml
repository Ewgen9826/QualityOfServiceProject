        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        
        <!--Материальность-->
        <materialDesign:Card materialDesign:ShadowAssist.ShadowDepth="Depth1" 
                             Padding="20" Margin="20" HorizontalAlignment="Stretch" 
                             VerticalAlignment="Stretch"
                             Background="#fff"
                             >
            <StackPanel Orientation="Vertical" 
                        HorizontalAlignment="Center" 
                        VerticalAlignment="Center">
                <TextBlock Text="Материальность" FontWeight="Bold" Foreground="{StaticResource PrimaryHueDarkBrush}"
                           Style="{StaticResource MaterialDesignSubheadingTextBlock}"
                           HorizontalAlignment="Center"/>
                <StackPanel Orientation="Horizontal" Margin="0 10">
                    
                    <StackPanel Orientation="Vertical">
                        <TextBlock Text="Ожидание" HorizontalAlignment="Center"
                               Style="{StaticResource MaterialDesignBody2TextBlock}"/>
                        <ProgressBar Style="{StaticResource MaterialDesignCircularProgressBar}"
                                     Value="80" HorizontalAlignment="Center"
                                     Height="50" Width="50" Margin="0 5"/>
                        <TextBlock Text="4.6" FontSize="20" HorizontalAlignment="Center"
                               Style="{StaticResource MaterialDesignBody2TextBlock}"/>
                    </StackPanel>

                    <StackPanel Orientation="Vertical" Margin="20 0">
                        <TextBlock Text="Восприятие" HorizontalAlignment="Center"
                               Style="{StaticResource MaterialDesignBody2TextBlock}"/>
                        <ProgressBar Style="{StaticResource MaterialDesignCircularProgressBar}"
                                     Foreground="#ffc107"
                                     Value="60" HorizontalAlignment="Center"
                                     Height="50" Width="50" Margin="0 5"/>
                        <TextBlock Text="3.4" HorizontalAlignment="Center" FontSize="20"
                               Style="{StaticResource MaterialDesignBody2TextBlock}"/>
                    </StackPanel>

                    <StackPanel Orientation="Vertical">
                        <TextBlock Text="Значимость" HorizontalAlignment="Center"
                               Style="{StaticResource MaterialDesignBody2TextBlock}"/>
                        <ProgressBar Style="{StaticResource MaterialDesignCircularProgressBar}"
                                     Foreground="#ef5350"
                                     Value="40" HorizontalAlignment="Center"
                                     Height="50" Width="50" Margin="0 5"/>
                        <TextBlock Text="2.9" HorizontalAlignment="Center" FontSize="20"
                               Style="{StaticResource MaterialDesignBody2TextBlock}"/>
                    </StackPanel>


                </StackPanel>
                <ComboBox materialDesign:HintAssist.Hint="Выбор подкритерия" IsReadOnly="True"
                          IsEditable="True" Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                          HorizontalAlignment="Stretch" Margin="0 10 0 0" FontSize="16">
                    <ComboBoxItem IsSelected="True">
                        Общие показатели
                    </ComboBoxItem>
                    <ComboBoxItem>
                        М1
                    </ComboBoxItem>
                    <ComboBoxItem>
                        М2
                    </ComboBoxItem>
                    <ComboBoxItem>
                        М3
                    </ComboBoxItem>
                </ComboBox>
            </StackPanel>
